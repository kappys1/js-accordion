{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/standard",
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "allowTernary": true

  }
}
